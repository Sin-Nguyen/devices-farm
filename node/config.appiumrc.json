{
 "server": {
  "allow-cors": true,
  "base-path": "/wd/hub",
  "debug-log-spacing": true,
  "keep-alive-timeout": 30000,
  "local-timezone": true,
  "log-timestamp": true,
  "long-stacktrace": true,
  "relaxed-security": true,
  "session-override": true,
  "use-drivers": [
   "uiautomator2",
   "xcuitest"
  ],
  "use-plugins": [
   "device-farm",
   "appium-dashboard"
  ],
  "plugin": {
   "device-farm": {
    "platform": "android",
    "deviceTypes": "both",
    "skipChromeDownload": true,
    "maxSessions": 1
   }
  }
 }
}